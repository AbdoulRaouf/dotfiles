    "on-click": "activate",
    "sort-by-number": true,
  },
  "tray": {
    "spacing": 10,
  },
  "clock#cl2": {
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "weeks-pos": "right",
