{
  "pyprland": {
    "plugins": [
      "scratchpads",
      "expose",
      "magnify"
    ]
  },
  "scratchpads": {
    "volume": {
      "command": "pavucontrol",
      "unfocus": "hide",
      "animation": "fromLeft",
      "margin": 20
    },
    "terminal": {
      "command": "kitty --class scratchpad"
    },
    "yazi": {
      "command": "kitty --class scratchpad yazi"
    },
    "btop": {
      "command": "kitty --class scratchpad btop"
    }
  }
}
