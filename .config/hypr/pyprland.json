{
  "pyprland": {
    "plugins": [
      "scratchpads",
      "expose",
      "magnify"
    ]
  },
  "scratchpads": {
    "volume": {
      "command": "pavucontrol",
      "unfocus": "hide",
      "animation": "fromLeft",
      "margin": 20,
      "lazy": true
    },
    "terminal": {
      "command": "kitty --class scratchpad"
    },
    "yazi": {
      "command": "kitty --class scratchpad yazi"
    },
    "gpt": {
      "command": "kitty --class scratchpad tgpt -i",
      "lazy": true
    },
    "dots": {
      "command": "kitty --class scratchpad yadm enter lazygit",
      "lazy": true
    },
    "btop": {
      "command": "kitty --class scratchpad btop",
      "lazy": true
    },
    "bluetooth": {
      "command": "blueman-manager",
      "animation": "fromRight",
      "unfocus": "hide",
      "margin": 20,
      "lazy": true
    }
  }
}
